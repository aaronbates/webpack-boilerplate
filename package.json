{
  "name": "webpack-boilerplate",
  "homepage": "https://github.com/aaronbates/webpack-boilerplate",
  "description": "Boilerplate for webpack projects.",
  "keywords": [
    "babel",
    "css",
    "express",
    "javascript",
    "react",
    "sass",
    "webpack"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "private": "false",
  "author": "Aaron Bates <aaron@aaronbates.me> (http://aaronbates.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronbates/webpack-boilerplate.git"
  },
  "main": "client/index.js",
  "scripts": {
    "clean": "npm-run-all clean:client clean:server",
    "clean:client": "rm -rf public/**",
    "clean:server": "rm -rf dist/**",
    "lint:css": "stylelint './client/css/**/*.css' --color",
    "lint:sass": "stylelint './client/scss/**/*.scss' --color --syntax scss",
    "lint:client": "eslint ./client/js --color --ext .jsx,.js",
    "lint:server": "eslint ./server/js --color --ext .jsx,.js",
    "lint:webpack": "eslint webpack.*.js postcss.*.js --color",
    "test": "",
    "debug": "npm run debug:client",
    "debug:client": "cross-env DEBUG=true webpack",
    "debug:build": "cross-env NODE_ENV=production DEBUG=true webpack",
    "start": "npm run start:client",
    "start:client": "webpack-dev-server",
    "start:server": "webpack",
    "start:all": "npm-run-all --parallel start:client start:server",
    "browsersync": "webpack-dev-server",
    "storybook": "start-storybook -p 9001 -c client/.storybook",
    "build": "npm run build:client",
    "build:client": "npm run clean:client && cross-env NODE_ENV=production webpack",
    "build:server": "npm run clean:server && babel ./server -d dist --presets node6 --copy-files",
    "build:all": "npm-run-all build:client build:server",
    "serve": "NODE_ENV=production node dist/server.js",
    "preversion": "npm test",
    "version": "",
    "postversion": ""
  },
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "config": "^1.28.1",
    "design-system-utils": "^0.11.1",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "lazysizes": "4.0.1",
    "object-assign": "4.1.1",
    "picturefill": "3.0.2",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-document-meta": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-ga": "^2.3.5",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.1.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-preset-node6": "11.0.0",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.21.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-hot-loader": "^1.3.5",
    "css-literal-loader": "^0.2.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "redux": "^3.7.2",
    "enzyme": "^3.2.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "jest": "^22.0.3",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "node-sass-magic-importer": "^5.0.3",
    "npm-run-all": "^4.1.2",
    "offline-plugin": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-bem-linter": "^3.1.0",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "postcss-sorting": "^3.1.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "redux-devtools": "^3.4.1",
    "reload-html-webpack-plugin": "^0.1.2",
    "sass-loader": "^6.0.6",
    "shevy": "^2.1.0",
    "shevyjs": "^1.1.0",
    "start-server-webpack-plugin": "^2.2.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-node-externals": "^1.6.0",
    "webpack-plugin-critical": "^1.0.0",
    "webpack-merge": "^4.1.1",
    "webpack-shell-plugin": "^0.5.0",
    "@storybook/react": "^3.3.3",
    "@storybook/addon-notes": "^3.3.3",
    "@storybook/addon-options": "^3.3.3"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "iOS >= 8",
    "not ie <= 8"
  ]
}
